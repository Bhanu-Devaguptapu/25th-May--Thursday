#include<stdio.h>
main(){
	char str[100],lower=0,special=0,upper=0,i;
	printf("Enter the string:");
	gets(str);
	for(i=0;str[i]!='\0';i++)
	{
		if(str[i]>='a' && str[i]<='z')
			lower++;
		else if(str[i]>='A' && str[i]<='Z')
		upper++;
		else
			special++;
		}
		printf("LOWER CASE:%d",lower);
		printf("\nUPPER CASE:%d",upper);
		printf("\nSPECIAL SYMBOL:%d",special);
}
